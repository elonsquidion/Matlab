ut = 0.000000001;
g = 9.8;
rhop = 1200;
rho = 1000;
Dp = 5E-3;
miu = 0.001;
Cd = 0;

for i = 1:100
    Nre = (rho*ut*Dp)/miu;

    if (Nre < 0.2)
        Cd = 24/Nre;
    elseif (Nre < 500)
        Cd = (24/Nre)*(1+0.5*Nre^0.687);
    elseif (Nre < 200000)
        Cd = 0.44;
    else
        Cd = 0.1;
    end

    a = ut - sqrt((4*g*(rhop-rho)*Dp)/(3*Cd*rho));

    fprintf("Nre = %f, Cd = %f, ut = %f\n", Nre, Cd,ut);
end

